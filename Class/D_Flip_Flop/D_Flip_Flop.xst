set -tmpdir "./xst/projnav.tmp"
set -xsthdpdir "./xst"
run
-ifn D_Flip_Flop.prj
-ifmt mixed
-ofn D_Flip_Flop
-ofmt NGC
-p acr2
-top D_Flip_Flop
-opt_mode Speed
-opt_level 1
-iuc NO
-lso D_Flip_Flop.lso
-keep_hierarchy YES
-rtlview Yes
-hierarchy_separator /
-bus_delimiter <>
-case maintain
-verilog2001 YES
-fsm_extract YES -fsm_encoding Auto
-safe_implementation No
-mux_extract YES
-resource_sharing YES
-iobuf YES
-pld_mp YES
-pld_xp YES
-pld_ce YES
-wysiwyg NO
-equivalent_register_removal YES
